<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping xmlns="http://castor.exolab.org/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://castor.exolab.org/mapping.xsd">
    
    <class name="gov.nih.nci.ncicb.cadsr.bulkloader.excel.transformer.beans.ExcelForm">
        <map-to xml="Form" ns-prefix="ns1" ns-uri="urn:nci:caDSR"/>
        <field name="formName" type="string">
            <bind-xml name="ns1:formName" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="context" type="string">
            <bind-xml name="ns1:context" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="classScheme" type="string">
            <bind-xml name="ns1:cs" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="classSchemeItem" type="string">
            <bind-xml name="ns1:csi" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="source" type="string">
            <bind-xml name="ns1:source" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="defaultConceptualDomain" type="string">
            <bind-xml name="ns1:defaultCD" xmlns:ns1="urn:nci:caDSR" />
        </field>
        <field name="questions" collection="arraylist" type="gov.nih.nci.ncicb.cadsr.bulkloader.excel.transformer.beans.ExcelQuestion" >
            <bind-xml name="Question" location="questions" />
        </field>
    </class>
    <class name="gov.nih.nci.ncicb.cadsr.bulkloader.excel.transformer.beans.ExcelQuestion">
        <map-to xml="Question" ns-prefix="ns1" ns-uri="urn:nci:caDSR"/>
        <field name="moduleName" type="string">
            <bind-xml name="ns1:moduleName" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="questionNumber" type="integer">
            <bind-xml name="ns1:questionNumber" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="alternateName" type="string">
            <bind-xml name="ns1:alternateName" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="alternateNameType" type="string">
            <bind-xml name="ns1:alternateNameType" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="preferredQuestion" type="string">
            <bind-xml name="ns1:preferredQuestion" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="alternateQuestion" type="string">
            <bind-xml name="ns1:alternateQuestion" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="questionNotes" type="string">
            <bind-xml name="ns1:questionNotes" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="cdeId" type="string">
            <bind-xml name="ns1:cdeId" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="cdeLongName" type="string">
            <bind-xml name="ns1:cdeLongName" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="cdeNotes" type="string">
            <bind-xml name="ns1:cdeNotes" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="decId" type="string">
            <bind-xml name="ns1:decId" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="ocQualConcepts" type="string">
            <bind-xml name="ns1:ocQualConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="ocPrimConcepts" type="string">
            <bind-xml name="ns1:ocPrimConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="propQualConcepts" type="string">
            <bind-xml name="ns1:propQualConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="propPrimConcepts" type="string">
            <bind-xml name="ns1:propPrimConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="decConceptualDomainId" type="string">
            <bind-xml name="ns1:decConceptualDomainId" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="decNotes" type="string">
            <bind-xml name="ns1:decNotes" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="vdId" type="string">
            <bind-xml name="ns1:vdId" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="repTermQualConcepts" type="string">
            <bind-xml name="ns1:repTermQualConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="repTermPrimConcepts" type="string">
            <bind-xml name="ns1:repTermPrimConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="vdConceptualDomainId" type="string">
            <bind-xml name="ns1:vdConceptualDomainId" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="dataType" type="string">
            <bind-xml name="ns1:dataType" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="vdMaxLength" type="string">
            <bind-xml name="ns1:vdMaxLength" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="enumerated" type="string">
            <bind-xml name="ns1:enumerated" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="pv" type="string">
            <bind-xml name="ns1:pv" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="pvLength" type="string">
            <bind-xml name="ns1:pvLength" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="vmConcepts" type="string">
            <bind-xml name="ns1:vmConcepts" xmlns:ns1="urn:nci:caDSR"/>
        </field>
        <field name="vmNotes" type="string">
            <bind-xml name="ns1:vmNotes" xmlns:ns1="urn:nci:caDSR"/>
        </field>
    </class>
</mapping>